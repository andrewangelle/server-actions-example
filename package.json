{
  "name": "vinxi-server-actions",
  "type": "module",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "vinxi start",
    "dev": "vinxi dev",
    "dev:vite": "vite",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "build:clean": "rm -rf dist",
    "build:app": "tsc && vinxi build",
    "build": "pnpm build:clean && npm run build:app",
    "lint:fix": "pnpm biome check app --apply",
    "lint:check": "pnpm biome check app",
    "prepare": "husky",
    "pre-commit": "lint-staged",
    "format": "pnpm biome format app --write"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "pnpm lint:fix",
      "pnpm format"
    ]
  },
  "dependencies": {
    "@picocss/pico": "^1.5.10",
    "@vinxi/react": "0.2.2",
    "@vinxi/react-server-dom": "0.0.3",
    "@vinxi/server-components": "0.3.3",
    "@vinxi/server-functions": "0.3.2",
    "@vitejs/plugin-react": "^4.0.4",
    "acorn-loose": "^8.3.0",
    "autoprefixer": "^10.4.15",
    "react": "0.0.0-experimental-035a41c4e-20230704",
    "react-dom": "0.0.0-experimental-035a41c4e-20230704",
    "tailwindcss": "^3.3.3",
    "unstorage": "^1.10.1",
    "vinxi": "0.3.10"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.6.2",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2"
  }
}
